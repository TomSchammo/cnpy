@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/cnpyTargets.cmake")

set(cnpy_INCLUDE_DIRS "@PACKAGE_INCLUDE_INSTALL_DIR@")
set(cnpy_LIBRARIES cnpy)

if(NOT TARGET cnpy::cnpy)
    add_library(cnpy::cnpy ALIAS cnpy)
endif()

# Provide the include directories to the consuming project
set_property(TARGET cnpy::cnpy PROPERTY INTERFACE_INCLUDE_DIRECTORIES ${cnpy_INCLUDE_DIRS})
